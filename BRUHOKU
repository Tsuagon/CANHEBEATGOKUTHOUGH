aa21e.MeshType = "Sphere"
aa21e.Scale = Vector3.new(1,1,1)

	for i = 1,25 do
	wait()
	aa21e.Scale = aa21e.Scale + Vector3.new(5,5,5)
	a21e.Transparency = a21e.Transparency + 0.04
	end
	
	wait(.1)
	
	a21e:Remove()
	
	wait()

hum.WalkSpeed = 55
