	if key == "r" then
			if ssj1 == true and being == false and ssj2 == true and kaioken == false and ssj3 == false and assj2 == false and ssjb == false and ssjwhite == false and ssjbfullpower == false and ultrainstinct == false then
			ass2()
		end
	end
end)

